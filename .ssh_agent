
set PROC = ( `sockstat -u | grep -m 1 -E "$USER.+ssh-agent" | cut -w -f 3,6` )

if ( "x$PROC" == "x" ) then
    eval `ssh-agent`
else
    setenv SSH_AGENT_PID $PROC[1]
    setenv SSH_AUTH_SOCK $PROC[2]
endif


